<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="MySampleApp">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.github.gwtbootstrap.Bootstrap"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='ru.study.gwttask.mySampleApp.client.MySampleApp'/>
    
    <extend-property name="locale" values="ru"/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/DBService' class='ru.study.gwttask.mySampleApp.server.DBServiceImpl'/>

    <source path='client'/>
    <source path='shared'/>
</module>